<link rel="stylesheet" type="text/css" href="public/css/bootstrap.min.css" />
<link rel="stylesheet" type="text/css" href="public/css/screen.css" />
<link rel="stylesheet" type="text/css" href="public/css/mobile.css" />
<style>
.table>tbody>tr:first-child>td {
    border-top: none;
}
</style>
<div id="page">
{{#bio_data}}
	<div id="top-contact">
		<div class="contact-details" style="text-align: center;">
			{{#email}}
			<div class="icon-envelope icon-square"></div> <a href="mailto:{{email}}" target="_top">{{email}}</a><br />
			{{/email}}
			{{#phone}}
			<div class="icon-phone icon-square"></div> {{phone}}<br />
			{{/phone}}
			{{#website}}
			<a href="{{website}}"> {{website}}</a>
			{{/website}}
		</div>
	</div>
	
	<h1 class="emphnext">{{firstname}} {{familyname}}</h1>
	{{#stars}}
	<span class="stars" style="clear:left">
		 {{#items}}
		 {{name}} {{^last}}<div class="icon-star-o icon-star"></div>{{/last}}
		 {{/items}}
	</span>
	{{/stars}}
	<br/><br/>

	{{#summary}}
	<h2><div class="icon-user icon-square"></div> Qualifications Summary</h2>
	<ul>
		{{#points}}
		<li>{{{.}}}</li>
		{{/points}}
	</ul>
	{{/summary}}

	{{#github_projects}}
	<h2><div class="icon-github icon-square"></div> ##github_projects##</h2>
	<ul>
	{{#details}}
		<li><a href="https://github.com/{{project_name}}">{{project_name}}</a>
		{{#tagline}} &raquo; {{{tagline}}}{{/tagline}}
			<ul>
				{{#description}}
				<li>{{{.}}}</li>
				{{/description}}
			</ul>
		</li>
	{{/details}}
	</ul>
	{{/github_projects}}

	{{#skills}}
	<h2><div class="icon-th-list icon-square"></div> Technical Skills</h2>
		<table class="table" style="text-align: center; font-size: 14px;">
		{{#details}}
			{{#items}}
			 <tr>
			 {{#name}}
				<td class="col-md-2">{{.}}</td>
			 {{/name}}
			 </tr>
			{{/items}}
		{{/details}}
		</table>
	{{/skills}}

	{{#experience}}
	<h2><div class="icon-briefcase icon-square"></div> Professional Experience</h2>
	<ul>
		{{#items}}
		<li>{{title}}, {{{organisation}}}{{#location}}, {{{location}}}{{/location}}
			<span class="right">({{from}} – {{to}})</span>
			<ul>
				{{#details}}
				<li>{{{.}}}</li>
				{{/details}}
				{{#technology_used}}
				<li>##technology_used##: 
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}
			</ul>
		</li>
		{{/items}}
	</ul>
	{{/experience}}

	{{#volunteer_work}}
	<h2><div class="icon-briefcase icon-square"></div> Volunteer Work</h2>
	<ul>
		{{#items}}
		<li>{{title}}, {{{organisation}}}
			<span class="right">({{from}} – {{to}})</span>
			<ul>
				{{#details}}
				<li>{{{.}}}</li>
				{{/details}}
				{{#technology_used}}
				<li>##technology_used##: 
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}
			</ul>
		</li>
		{{/items}}
	</ul>
	{{/volunteer_work}}
	
	{{#education}}
	<h2><div class="icon-mortar-board icon-rect"></div> ##education##</h2>
	<div id="mobile-table">
		<div class="table-responsive">
			<table class="table education">
				<tr>
					<th>##degree##</th><th>##major##</th><th>##institution##</th><th nowrap>##graduation_year##</th>{{#show_gpa}}<th>##gpa##</th>{{/show_gpa}}
				</tr>
				{{#schools}}
				<tr>
					<td>{{degree}}</td><td>{{major}}</td><td>{{institution}}</td><td align="center">{{graduation_year}}</td>{{#show_gpa}}<td>{{gpa}}</td>{{/show_gpa}}
				</tr>
				{{/schools}}
			</table>
		</div>
	</div>
	<div id="desktop-table">
		<table class="table education">
			<tr>
				<th>##degree##</th><th>##major##</th><th>##institution##</th><th nowrap>##graduation_year##</th>{{#show_gpa}}<th>##gpa##</th>{{/show_gpa}}
			</tr>
			{{#schools}}
			<tr>
				<td>{{degree}}</td><td>{{major}}</td><td>{{institution}}</td><td align="center">{{graduation_year}}</td>{{#show_gpa}}<td>{{gpa}}</td>{{/show_gpa}}
			</tr>
			{{/schools}}
		</table>
	</div>
	{{/education}}
	
	{{#certifications}}
	<h2><div class="icon-th-list icon-square"></div>IT Certifications</h2>
		{{#details}}
			{{#items}}
				<div style="text-align: center;">
				{{#item1}}
					<span class="label label-default">{{.}}</span>
				{{/item1}}
				</div>
			 </tr>
			{{/items}}
		{{/details}}
	{{/certifications}}	
	
	<br />
	<div id="bottom-contact" text-size="80%">
		{{#email}}
		<div class="icon-envelope icon-square"></div> {{email}}<br />
		{{/email}}
		{{#phone}}
		<div class="icon-phone icon-square"></div> {{phone}}<br />
		{{/phone}}
		{{#website}}
		<div class="icon-globe icon-square"></div><a href="{{website}}"> {{website}}</a>
		{{/website}}
	</div>
</div>

{{/bio_data}}

{{#settings}}
	{{#icons}}
	<script src="public/js/grunticon.js"></script>
	{{/icons}}
{{/settings}}
</div><!-- End Page -->
